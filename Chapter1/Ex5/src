package cmc.edu.vn.chapter5;


import java.util.LinkedList;

public class Main {
    public static void main(String[] args) {
      
        LinkedList<Integer> queue = new LinkedList<>();

 
        queue.add(10);
        queue.add(15);
        queue.add(30);

       
        queue.addFirst(5);
        queue.addLast(40);

     
        queue.removeFirst();
        queue.removeLast();

    
        System.out.println("List: " + queue);
    }
}

