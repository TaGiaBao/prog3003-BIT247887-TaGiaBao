package cmc.edu.vn.chapter1;

class Test<T> {
    private T value;

    public void set(T t) {
        this.value = t;
    }

    public T get() {
        return this.value;
    }
}


public class Main {
    public static void main(String[] args) {
       
        Test<Integer> integerTest = new Test<>();
        integerTest.set(100);
        System.out.println("Giá trị Integer: " + integerTest.get());

       
        Test<String> stringTest = new Test<>();
        stringTest.set("Hello!");
        System.out.println("String: " + stringTest.get());
    }
}

