
package cmc.edu.vn.chapter6;


import java.util.HashSet;

public class Main {
    public static void main(String[] args) {
     
        int[] arr = {1,6,4,8,9,3,7};

   
        HashSet<Integer> set = new HashSet<>();
        for (int num : arr) {
            set.add(num);
        }

      
        System.out.println("Các phần tử duy nhất: " + set);

       
    }
}
